<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mcare"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/kidphoto"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mcare.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="com.example.mcare.AdviceFeedback"
            android:label="@string/app_name"  >
            
        </activity>
        
          <activity android:name="com.example.mcare.AdviceForm" >
            <intent-filter>
                <action android:name="com.example.mcare.ADVICEFORM" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
          <activity android:name="com.example.mcare.AppAvailForm" >
            <intent-filter>
                <action android:name="com.example.mcare.APPAVAILFORM" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
          <activity android:name="com.example.mcare.AppointmentForm" >
            <intent-filter>
                <action android:name="com.example.mcare.APPOINTMENTFORM" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
          <activity android:name="com.example.mcare.ChoiceForm" >
            <intent-filter>
                <action android:name="com.example.mcare.CHOICEFORM" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
          <activity android:name="com.example.mcare.PatientsProfile" >
            <intent-filter>
                <action android:name="com.example.mcare.PATIENTSPROFILE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
          <activity android:name="com.example.mcare.RegistrationForm" >
            <intent-filter>
                <action android:name="com.example.mcare.REGISTRATIONFORM" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
          
    </application>

</manifest>